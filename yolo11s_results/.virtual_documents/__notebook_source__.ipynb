get_ipython().getoutput("pip install ultralytics")
from ultralytics import YOLO

# Load a pretrained YOLO11s model
model = YOLO("yolo11s.pt")

# Train the model on the dataset for 100 epochs
train_results = model.train(
    data="/kaggle/input/odpady-klasyfikacja/data.yaml",  # Path to dataset configuration file
    epochs=100,  # Number of training epochs
    imgsz=640,  # Image size for training
    device=0,  # Device to run on (e.g., 'cpu', 0, [0,1,2,3])
    patience=10
)

# Evaluate the model's performance on the validation set
metrics = model.val()

# Export the model to ONNX format for deployment
path = model.export(format="onnx")  # Returns the path to the exported model
